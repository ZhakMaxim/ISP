<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ISP_253504_Zhak.FourthLabPage"
             Title="FourthLabPage">
    <VerticalStackLayout>
         <CollectionView x:Name="CollView" Grid.Row="1" >
     <CollectionView.ItemTemplate>
         <DataTemplate>
             <Frame HeightRequest="50" WidthRequest="300" Margin="0,0,0,10">
             <Label Text="{Binding Cur_Name}" HeightRequest="20"></Label>
             </Frame>
         </DataTemplate>
     </CollectionView.ItemTemplate>
 </CollectionView>
        <Button Text="button" x:Name="btn" Clicked="OnBtnClicked"></Button>
    </VerticalStackLayout>
</ContentPage>